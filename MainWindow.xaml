<Window x:Class="maze_iteration_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:maze_iteration_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="800">

    <Grid>
        <Canvas x:Name="mycanvas" Background="MediumPurple">
            <StackPanel>
                <Button
                    Click="generate_click"
                    Width="100"
                    Height="50"
                    Margin="650,100,0,0"
                    BorderBrush="Black">generate</Button>
            </StackPanel>

        </Canvas>

    </Grid>
</Window>